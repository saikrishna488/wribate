{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/app/page.jsx", "app/components/Sidebar.jsx", "app/components/Navbar.jsx", "app/admin/blogs/post-blog/page.jsx", "app/admin/blogs/page.jsx", "app/admin/login/page.jsx", "app/components/admin/Navbar.jsx", "app/admin/page.jsx", "app/admin/layout.jsx", "app/blogs/[id]/page.jsx", "app/blogs/page.jsx", "app/wribate/[id]/page.jsx", "app/propose-wribate/page.js", "app/my-wribates/components/MyAssignedTopics/MyAssignedTopics.tsx", "app/my-wribates/components/MyAssignedTopics/CreateEditArticle.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
